@model IEnumerable<EventTracker.BusinessModel.Event>

@{
    ViewBag.Title = "title";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <div class="row">
        @if (Model.Any())
        {
            <hr/>
            <table class="table table-hover table-striped" id="eventsTable">
                <thead>
                <tr>
                    <th>ID</th>
                    <th>Date</th>
                    <th>Name</th>
                </tr>
                </thead>
                <tbody>
                    @foreach (var anEvent in Model) {
                        <tr>
                            <td>@anEvent.EventId</td>
                            <td>@anEvent.EventDate</td>
                            <td>@anEvent.EventName</td>
                    </tr>
                    }
                </tbody>
            </table>
        }

    </div>

</div>